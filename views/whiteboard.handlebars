<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/logout">
        Logout
      </a>
    </div>
  </div>
</nav>
<div class="container">
  <div class="row">
    {{#each stickyNote}}
    <div id="{{ this.id }}" class="draggable thisMoves"
      style="transform: translate({{ this.xCoord }}px, {{ this.yCoord }}px);" data-x="{{ this.xCoord }}"
      data-y="{{ this.yCoord }}">

      <div class="deleteButton"><i class="fas fa-times"></i></div>
      <div class="voteButton"><i class="fas fa-star"></i>
        <p class="voteCount"></p>
      </div>
      <p class="textInput" contentEditable="true"> {{ this.noteText }} </p>

      <p contentEditable="true"> {{ this.noteText }} </p>
    </div>
    {{/each}}

    <div class="side-panel">
      <div class="side-panel-icon">
        <img src="/assets/img/add-post-it.svg" id="new-post">
      </div>
      <div class="side-panel-icon">
        <img src="/assets/img/new-comment.svg" id="new-comment">
      </div>
      <div class="side-panel-icon">
        <img src="/assets/img/star.svg" id="star">
      </div>
    </div>
  </div>
</div>